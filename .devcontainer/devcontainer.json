{
  "name": "Only Jupyter",
  "image": "mcr.microsoft.com/devcontainers/universal:latest",
  "remoteUser": "vscode",
  "hostRequirements": {
    "cpus": 4
  },
  "features": {
    "ghcr.io/devcontainers/features/python": {
      "version": "3.10",
      "installJupyterlab": true,
      "configureJupyterlabAllowOrigin": "*"
    }
  }
}
